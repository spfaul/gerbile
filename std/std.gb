proc print in str text dec
    int len = call strlen text end
    call scall3 1 1 text len end
end

proc println in str text dec
    call print text end
    call print "\n" end
end

proc close in int fd dec
    call scall1 3 fd end
end

proc write in int fd dec str s dec
    call scall3 1 fd s call strlen s end end
end

proc open in str s dec
    int fd = call scall3 2 s 65 420 end
    ret fd
end
