include "std/std.gb"

proc main in
    int i = 1
    loop i < 20 do
        call print_int call fib i end end
        call print " " end
        i = i add 1
    end
    call print "\n" end
    ret 0
end

proc fib in int n dec
    if n < 2 do
        ret n
    end else do
        ret call fib n sub 1 end add call fib n sub 2 end
    endif
end