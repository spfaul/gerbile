include "std/std.gb"
macro FIZZBUZZ_CAP 20

proc main in
    int i = 0
    loop i < FIZZBUZZ_CAP do
        if i mod 15 == 0 do
            call println "FizzBuzz" end
        end elif i mod 3 == 0 do
            call println "Fizz" end
        end elif i mod 5 == 0 do
            call println "Buzz" end
        endif
        i = i add 1
    end
    ret 0
end
